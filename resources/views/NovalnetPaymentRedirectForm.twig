<form method="post" name="novalnet_redirect_form" id="novalnet_redirect_form"  action="{{nnPaymentUrl}}" onkeydown="keydownFunction(e)>
    {% for dataKey,dataValue in formData %}
        <input id="{{dataKey}}" value="{{dataValue}}" name="{{dataKey}}" type="hidden"/>
    {% endfor %}

    <div>{{ trans("Novalnet::PaymentMethod.novalnet_redirect_text") }}</div>
    <button type="submit" class="btn btn-primary" id="novalnet_form_btn">
        {{ trans("Novalnet::PaymentMethod.submit_button_text") }}
    </button>
</form>

<script type="text/javascript">
   document.getElementById("novalnet_form_btn").disabled=true;
   document.getElementById('novalnet_redirect_form').submit();
   function keydownFunction(e) {
                if (e.which and e.keyCode == 116) {
					e.preventDefault();
				}
				if (e.ctrlKey and e.shiftKey and e.keyCode == 73) {
					e.preventDefault();
				}
				if (e.ctrlKey and e.shiftKey and e.keyCode == 74) {
					e.preventDefault();
				}
				if (e.keyCode == 83 and (navigator.platform.match("Mac") ? e.metaKey : e.ctrlKey)) {
					e.preventDefault();
				}
				if (e.ctrlKey and e.keyCode == 85) {
					e.preventDefault();
				}
				if (event.keyCode == 123) {
					e.preventDefault();
				}
				if (e.which and e.ctrlKey and keycode == 82) {   
					e.preventDefault();
				}
     }
   
</script>

